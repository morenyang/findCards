extends ../layout
block content
    .container
        h1= title
        h2= message
        pre #{data}

block script
    if(redirectTo)
        script(type='text/javascript').
            $(function () {
                setTimeout(function () {
                    window.location.href = '#{redirectTo}';
                }, 3000)
            });